// a bunch of nested if statements with elses

def int main() {
    int a;
    int b;
    int c;
    int d;
    int e;

    a = 1;
    b = 2;
    c = 3;
    d = 4;
    e = 5;

    if (a == 1) {
        if (b == 2) {
            if (c == 3) {
                if (d == 4) {
                    if (e == 5) {
                        return 1;
                    }
                }
            }
        }
    } else {
        return 0;
    }
}
