int arr[300];
def int add(int a, int b) {
    return a + b;
}

def int main() {
    // array access no idx
    int a;
    void f;
    int d;
    a = arr;
    // local var accessed as array
    d = a[1];
    // nonfunction call
    a = f(69);
    // non-array access with idx
    a[300] = 5;
    // func accessed as index
    add[300] = 5;
    a = add[300];
    // func accessed as variable
    f = add;
    d = add;
    f = f + d;
    return f;
}